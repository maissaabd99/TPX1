<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TPX1.Page4">
    <TabbedPage.Children>
        <ContentPage Title="Home" BackgroundColor="Aqua">
            <Label Text="Home page" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
        </ContentPage>
        <ContentPage Title="Contact" BackgroundColor="Red">
            <Label Text="Contact Page" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
        </ContentPage>
        <ContentPage Title="About Us" BackgroundColor="Beige">
            <Label Text="About Us" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
        </ContentPage>
    </TabbedPage.Children>
</TabbedPage>